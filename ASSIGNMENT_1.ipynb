      "source": [
        "# function to find largest in the list using for loop\n",
        "def large(list1):\n",
        "  max = list1[0]\n",
        "  for x in list1:\n",
        "    if x > max:\n",
        "      max = x\n",
        "\n",
        "  return max\n",
        "\n",
        "list1 = [5, 10, 15, 75]\n",
        "print(\"Largest element is:\", large(list1))"
      ]
    }
  ]
